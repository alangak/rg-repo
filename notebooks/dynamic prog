T2B","executionInfo":{"status":"ok","timestamp":1595776638096,"user_tz":-330,"elapsed":1936,"user":{"displayName":"Rahul Gulabani","photoUrl":"","userId":"12132587162706927266"}},"outputId":"dd9883bf-b875-4fed-9155-0ded1acd3ce6","colab":{"base_uri":"https://localhost:8080/","height":35}},"source":["#print length of longest common subsequence....using memoization\n","n=6;m=5\n","t=[[-1 for i in range(n+1)] for i in range(m+1)] \n","def LCS(s1,s2,m,n):\n","  if(n==0 or m==0):\n","    t[m][n]=0\n","    return t[m][n]\n","  if(t[m][n]!=-1):\n","    return t[m][n] \n","  if(s1[m-1]==s2[n-1]):\n","    t[m][n]= 1+ LCS(s1,s2,m-1,n-1)\n","    return t[m][n]\n","  else:\n","    t[m][n] =max(LCS(s1,s2,m-1,n),LCS(s1,s2,m,n-1))\n","    return t[m][n]\n","s1=\"acbcf\"\n","s2=\"abcdaf\"\n","m=len(s1)\n","n=len(s2)\n","LCS(s1,s2,5,6)      "],"execution_count":null,"outputs":[{"output_type":"execute_result","data":{"text/plain":["4"]},"metadata":{"tags":[]},"execution_count":14}]},{"cell_type":"code","metadata":{"id":"yxcHXXyVkg_v","executionInfo":{"status":"ok","timestamp":1595856342936,"user_tz":-330,"elapsed":6726,"user":{"displayName":"Rahul Gulabani","photoUrl":"","userId":"12132587162706927266"}},"outputId":"62856009-6dda-4df6-e9ad-e8d1dcd1b3df","colab":{"base_uri":"https://localhost:8080/","height":35}},"source":["#print length of longest common subsequence....using top down\n","\n","def LCS(s1,s2,m,n):\n","  \n","  t=[[-1 for i in range(n+1)] for i in range(m+1)] \n","  for i in range(m+1):\n","    for j in range(n+1):\n","      if(i==0 or j==0):\n","        t[i][j]=0\n","  for i in range(1,m+1):\n","    for j in range(1,n+1):\n","      if(s1[i-1]==s2[j-1]):\n","        t[i][j]= 1+t[i-1][j-1]\n","    \n","      else:\n","        t[i][j] =max(t[i-1][j],t[i][j-1])\n","  return t[m][n]\n","s1=\"acbcf\"\n","s2=\"abcdaf\"\n","s3=\"abcde\"\n","s4=\"abfce\"\n","m=len(s1)\n","n=len(s2)\n","LCS(s1,s2,5,6)      "],"execution_count":null,"outputs":[{"output_type":"execute_result","data